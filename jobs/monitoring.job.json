{
  "name": "mas-monitoring-job",
  "description": "Hourly metrics collection",
  "schedule": {
    "quartz_cron_expression": "0 0 * * * ?",
    "timezone_id": "UTC",
    "pause_status": "UNPAUSED"
  },
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "monitoring_task",
      "description": "Collect metrics from all agents",
      "notebook_task": {
        "notebook_path": "/Workspace/Repos/aiops-agentic-mas-platform/notebooks/08_llmops_monitoring/01_monitoring_dashboard",
        "source": "WORKSPACE"
      },
      "timeout_seconds": 1800,
      "max_retries": 2
    }
  ],
  "timeout_seconds": 1800,
  "format": "MULTI_TASK"
}

